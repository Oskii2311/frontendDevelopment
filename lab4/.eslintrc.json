{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": false
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 5
    },
    "rules": {
        "semi": "error",
        "no-console": "off"
    }
}
